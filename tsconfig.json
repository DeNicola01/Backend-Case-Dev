{
  "compilerOptions": {
    "target": "es2020",
    "module": "NodeNext",
    "isolatedModules": true, // changed to NodeNext to match moduleResolution
    "moduleResolution": "nodenext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
